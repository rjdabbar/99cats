<h1><%= @cat.name %></h1>

<h2>Cat Data!</h2>
<table>
  <tr>
    <td>name</td>
    <td>color</td>
    <td>age</td>
    <td>sex</td>
    <td>description</td>
  </tr>

  <tr>
    <td><%= @cat.name %></td>
    <td><%= @cat.color %></td>
    <td><%= @cat.age %></td>
    <td><%= @cat.sex %></td>
    <td><%= @cat.description %></td>
  </tr>
</table>

<h2>Cat Rental Requests</h2>
<table>
  <tr>
    <td>Start Date</td>
    <td>End Date</td>
  </tr>
<% @cat_requests.each do |request| %>

  <tr>
    <td> <%= request.start_date %> </td>
    <td> <%= request.end_date %> </td>
  </tr>
<% end %>
